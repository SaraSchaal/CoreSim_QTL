// file structure


new_data = "Gen " + "uniqueMuts " + "meanFitP1 " + "sdFitP1 " + "\n";
gen = c(20, 20, 40, 60);
uniMut = c(2,2,1,1);
meanFit = c(0.8, 0.8, 0.5, 0.6);
sdFit = c(0.01, 0.01, 0.06, 0.07);

for(i in 0:(size(gen)-1)){
	new_data = new_data + gen[i] + " " + uniMut[i] +
		 " " + meanFit[i] + " " + sdFit[i] + "\n";
}

str(new_data)
